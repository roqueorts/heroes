    "@angular/localize": "^17.1.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "json-concat": "^0.0.1",
    "json-server": "^1.0.0-alpha.22",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mockserver": "^3.1.1",
    "typescript": "~5.3.2"
